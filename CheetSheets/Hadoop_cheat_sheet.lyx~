\begin_layout Labeling
\labelwidthstring 00.00.0000
Hadoop
\begin_inset space ~
\end_inset

Streaming Allowing programming languages other than java to be used for
 map-reduce (
\series bold
bold
\series default
 = required)
\end_layout

\begin_layout Standard
\noindent
\begin_inset listings
lstparams "backgroundcolor={\color{lightgrey}},language=bash"
inline false
status open

\begin_layout Plain Layout

$ hadoop [--config confdir] jar ${HADOOP_HOME}/share/hadoop/tools/lib/hadoop-str
eaming-2.2.0.jar 
\backslash

\end_layout

\begin_layout Plain Layout

	[genericOptions] <streamingOptions>
\end_layout

\end_inset


\end_layout

