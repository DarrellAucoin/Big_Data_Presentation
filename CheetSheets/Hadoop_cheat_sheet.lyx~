\begin_layout Standard
Hadoop Streaming in R (pkg rmr2)
\end_layout

\begin_layout Standard
\begin_inset listings
lstparams "breaklines=true"
inline false
status open

\begin_layout Plain Layout

mapreduce(input, output = NULL, map = to.map(identity), reduce = NULL, vectorized.
reduce = FALSE, combine = NULL, 
\end_layout

\begin_layout Plain Layout

	in.memory.combine = FALSE, input.format = "native",   output.format = "native")
 
\end_layout

\end_inset


\end_layout

